{
    "manifest_version": 2,
    
    "name": "Jitsy styler",
    "description": "This extension will analyze a page using GTmetrix",
    "version": "1.0",
    
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_title": "Make this page red"
    },

    "permissions": [
        "activeTab"
    ]
}
